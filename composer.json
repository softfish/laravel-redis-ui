{
	"name" : "feikwok/laravel-redis-ui",
	"description" : "Redis content management system for laravel application",
	"keywords" : [
		"laravel",
		"redis",
		"ui"
	],
	"type" : "package",
	"require" : {
		"php" : ">=7.0",
		"illuminate/console" : "~5.3",
		"illuminate/database" : "~5.3",
		"illuminate/filesystem" : "~5.3",
		"illuminate/auth": "~5.3",
		"predis/predis" : "^1.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^4.0",
		"mockery/mockery" : "~0.9.4"
	},
	"license" : "MIT",
	"authors" : [{
			"name" : "Fei Kwok",
			"email" : "fkwok00@gmail.com"
		}
	],
	"minimum-stability" : "dev",
	"autoload" : {
		"psr-4" : {
			"Feikwok\\RedisUI\\" : "src/"
		}
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Feikwok\\RedisUI\\LaravelRedisUIServiceProvider"
			]
		}
	}
}
